<!-- BEGIN: main  -->
<!-- BEGIN: loop -->
<div class="nv-block-banners">
    <!-- BEGIN: type_image_link -->
    <a rel="nofollow" href="{DATA.link}" onclick="this.target='{DATA.target}'"  class="{DATA.id}" title="{DATA.file_alt}"><img class="{DATA.id}" alt="{DATA.file_alt}" src="{DATA.file_image}" width="{DATA.file_width}px"></a>
    <!-- END: type_image_link -->
    <!-- BEGIN: type_image -->
    <img alt="{DATA.file_alt}" class="{DATA.id}"  src="{DATA.file_image}" width="{DATA.file_width}px">
    <!-- END: type_image -->
    <!-- BEGIN: bannerhtml -->
    <div class="clearfix text-left bannerhtml">
        {DATA.bannerhtml}
    </div>
    <!-- END: bannerhtml -->
</div>
<!-- END: loop -->
<!-- END: main -->
